<h1>Create a new project2</h1>

<% if @project2.errors.any? %>
  <h2>The following errors prevented the project2 from being saved</h2>
  <ul>
   <% project2.errors.full_messages.each do |msg| %>
    <li><%msg%></li>
   <% end%>
   </ul>
<% end %>

<%= form_with scope: :project2, url: project2s_path, local: true do |f| %>

 <p>
   <%= f.lable :title %><br/>
   <%= f.text_field :title %>
 </p>

 <p>
   <%= f.lable :description %><br/>
   <%= f.text_area :description %>
 </p>

 <p>
   <%= f.submit %>
 </p>

<% end %>